  "scripts": {
    "preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions",
    "eslint:js": "git diff --name-only --diff-filter=dx origin/master... | grep js/.*\\.js | awk -F/js '{print \"js\"$2}' | xargs eslint -c .eslintrc.js",
    "eslint:js:fix": "npm run eslint:js -- --fix",
    "eslint:ts": "cd ts && eslint -c .eslintrc.js --ext .ts,.tsx --max-warnings=0 .",
    "eslint:ts:fix": "cd ts && eslint -c .eslintrc.js --ext .ts,.tsx --fix .",
    "stylelint": "stylelint --ignore-path .stylelintignore",
    "prettier": "prettier --write \"{js,ts}/**/*.{js,ts,tsx}\"",
    "prettier:check": "npm run prettier -- --list-different",
    "prepare": "cd ../../.. && husky install .husky",
    "tsc": "cd ts && tsc",
    "tsc:check": "npm run tsc -- --noEmit --skipLibCheck",
    "jest": "jest -c ts/jest.config.js",
    "jest:update": "npm run jest -- -u",
    "jest:watch": "npm run jest -- --watch",
    "jest:cov": "open ts/coverage/index.html",
    "mocha": "mocha --require @babel/register --require ts-node/register js/test/**/*.js",
    "webpack:watch": "webpack serve --config webpack.config.dev.js --env development --watch-options-stdin",
    "webpack:build": "webpack --config webpack.config.prod.js --env production",
    "webpack:build:react": "webpack --config webpack.config.react_app.js --env production",
    "webpack:analyze": "webpack --config webpack.config.prod.js --env production --profile --json > stats.json && webpack-bundle-analyzer stats.json ../priv/static/js/"
  },
  "lint-staged": {
    "{js,ts}/**/*.{js,ts,tsx}": "prettier --write",
    "js/**/*.js": "eslint -c .eslintrc.js --fix --max-warnings=0",
    "ts/**/*.{ts,tsx}": "cd ts && eslint -c .eslintrc.js --fix --max-warnings=0",
    "css/**/*.scss": "npm run stylelint -- --fix"
  }
